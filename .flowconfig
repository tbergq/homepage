[ignore]

[declarations]
.+/node_modules/.+

[include]

[libs]
./flow-typed/

[lints]
all=error
untyped-import=off
unclear-type=off

[options]
exact_by_default=true
experimental.const_params=true
experimental.types_first=true
experimental.well_formed_exports=true

module.file_ext=.js
module.file_ext=.json
module.file_ext=.md

[strict]
untyped-import
unclear-type